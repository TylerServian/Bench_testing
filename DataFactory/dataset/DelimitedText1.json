{
	"name": "DelimitedText1",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"directory": {
				"type": "string"
			},
			"file_name": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@concat(dataset().file_name, '.csv.gz')",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().directory",
					"type": "Expression"
				},
				"container": "raw"
			},
			"columnDelimiter": ",",
			"compressionCodec": "gzip",
			"compressionLevel": "Fastest",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": ""
		},
		"schema": []
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}